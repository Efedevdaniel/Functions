function validParentheses(
  str
) {
  const stack = [];
  const pairs = {
    ')': '(',
    ']': '[',
    '}': '{',
  };

  for (
    const char of str
  ) {
    if (pairs[char]) {
      if (
        stack.pop() !==
        pairs[char]
      ) {
        return false;
      }
    } else {
      stack.push(char);
    }
  }

  return stack.length === 0;
}
